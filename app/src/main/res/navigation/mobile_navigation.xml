<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.moneywizev1.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.moneywizev1.ui.dashboard.DashboardFragment"
        android:label="Categories"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.moneywizev1.ui.notifications.NotificationsFragment"
        android:label="Account"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/addFragment"
        android:name="com.example.moneywizev1.addFragment"
        android:label="@string/add"
        tools:layout="@layout/fragment_add" />
    <fragment
        android:id="@+id/contact"
        android:name="com.example.moneywizev1.contact"
        android:label="@string/contact"
        tools:layout="@layout/fragment_contact" />
</navigation>